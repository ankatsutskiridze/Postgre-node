generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model category {
    id          Int       @id @default(autoincrement())
    created_at  DateTime  @default(now()) @db.Timestamp(6)
    name        String?   @db.VarChar(255)
    description String?   @db.VarChar(255)
    products    products[]
}